{
  "editor.fontFamily": "JetBrains Mono, 'FiraCode Nerd Font', 'Fira Code', 'Menlo', 'Monaco', 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.insertSpaces": true,
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "explorer.confirmDelete": false,
  "files.autoSave": "onWindowChange",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "terminal.integrated.fontFamily": "JetBrains Mono, 'FiraCode Nerd Font', 'Fira Code', 'Menlo', 'Monaco', 'Courier New', monospace",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.minimumContrastRatio": 1,
  "vim.easymotion": true,
  "vim.handleKeys": {
    "<C-b>": true,
    "<C-d>": true,
    "<C-f>": true,
    "<C-u>": true
  },
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "after": ["<Esc>"],
      "before": ["j", "k"]
    }
  ],
  "vim.leader": "<space>",
  "vim.normalModeKeyBindings": [
    {
      "after": ["\"", "+", "y"],
      "before": ["<leader>", "y"]
    },
    {
      "after": ["\"", "+", "Y"],
      "before": ["<leader>", "Y"]
    },
    {
      "after": ["\"", "+", "p"],
      "before": ["<leader>", "p"]
    },
    {
      "after": ["\"", "+", "P"],
      "before": ["<leader>", "P"]
    },
    {
      "after": ["\"", "+", "d"],
      "before": ["<leader>", "d"]
    },
    {
      "after": ["\"", "+", "D"],
      "before": ["<leader>", "D"]
    },
    {
      "before": ["<leader>", "f", "s"],
      "commands": [":w"]
    },
    {
      "before": ["<leader>", "f", "S"],
      "commands": [":wa"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": [":FZF"]
    },
    {
      "before": ["<leader>", "b"],
      "commands": [":Buffers"]
    },
    {
      "before": ["<leader>", "h"],
      "commands": [":History"]
    },
    {
      "before": ["<leader>", "o", "t"],
      "commands": ["workbench.action.terminal.focus"]
    },
    {
      "before": ["<leader>", "."],
      "commands": ["workbench.action.quickOpenRecent"]
    }
  ],
  "vim.surround": true,
  "vim.useSystemClipboard": false,
  "vim.visualModeKeyBindings": [
    {
      "after": ["<Esc>"],
      "before": ["j", "k"]
    }
  ],
  "workbench.colorTheme": "Dracula Theme Soft",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cursor.composer.shouldChimeAfterChatFinishes": true,
  "search.useIgnoreFiles": false,
  "workbench.editor.enablePreview": false,
  "workbench.editor.showTabs": true,
  "cursor.experimental.tabIsolation": true
}
